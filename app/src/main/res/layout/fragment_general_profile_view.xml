<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".view.GeneralProfileView">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/primary_light"
        android:padding="20dp">


        <TextView
            android:id="@+id/textView5"
            android:layout_width="157dp"
            android:layout_height="37dp"
            android:background="@drawable/rounded_corner"
            android:backgroundTint="@color/primary_dark"
            android:padding="10dp"
            android:text="Additional Notes"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/primary_light"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/add_child_email"
            app:layout_constraintVertical_bias="0.033" />

        <Button
            android:id="@+id/add_child_save"
            android:layout_width="79dp"
            android:layout_height="34dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/rounded_corner"
            android:backgroundTint="@color/primary_dark"
            android:padding="5dp"
            android:text="Save"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/primary_light"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.128"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            app:layout_constraintVertical_bias="0.0" />


        <Button
            android:id="@+id/sign_out"
            android:layout_width="250dp"
            android:layout_height="34dp"
            android:layout_marginEnd="40dp"
            android:background="@drawable/rounded_corner"
            android:backgroundTint="@color/primary_dark"
            android:padding="5dp"
            android:text="Sign Out"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/primary_light"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.123"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:layout_constraintVertical_bias="0.065" />

        <EditText
            android:id="@+id/textView9"
            android:layout_width="157dp"
            android:layout_height="37dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/rounded_corner"
            android:backgroundTint="@color/primary_dark"
            android:ems="10"
            android:inputType="text"
            android:padding="10dp"
            android:text="Age"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/primary_text_light"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/textView8"
            app:layout_constraintVertical_bias="0.014" />


        <EditText
            android:id="@+id/add_child_email"
            android:layout_width="157dp"
            android:layout_height="37dp"
            android:layout_marginTop="1dp"
            android:background="@drawable/rounded_corner"
            android:backgroundTint="@color/primary_dark"
            android:ems="10"
            android:inputType="text"
            android:padding="10dp"
            android:text="Email"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/primary_text_light"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/textView9"
            app:layout_constraintVertical_bias="0.023" />

        <EditText
            android:id="@+id/textView8"
            android:layout_width="157dp"
            android:layout_height="37dp"
            android:layout_marginTop="3dp"
            android:layout_marginBottom="468dp"
            android:background="@drawable/rounded_corner"
            android:backgroundTint="@color/primary_dark"
            android:ems="10"
            android:inputType="text"
            android:padding="10dp"
            android:text="Name"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/primary_text_light"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/imageView2"
            app:layout_constraintVertical_bias="0.077" />

        <EditText
            android:id="@+id/textView6"
            android:layout_width="366dp"
            android:layout_height="205dp"
            android:background="@drawable/rounded_corner"
            android:backgroundTint="@color/primary_dark"
            android:ems="10"
            android:inputType="text"
            android:text="Please add your additional notes here"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textColor="@color/primary_text_light"
            android:textSize="13sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView2"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            app:layout_constraintVertical_bias="0.045" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="126dp"
            android:layout_marginTop="64dp"
            android:layout_marginEnd="16dp"
            android:background="#00E1D4E2"
            android:backgroundTint="#501414"
            android:text="My Provider"
            android:visibility="visible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.666"
            app:layout_constraintStart_toEndOf="@+id/imageView2"
            app:layout_constraintTop_toTopOf="parent"
            app:strokeColor="#401B1B" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="135dp"
            android:layout_height="121dp"
            android:layout_marginBottom="20dp"
            android:src="@drawable/profile_pic_icon"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.068"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.046" />


    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".profileView">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#1B3C53">

        <ImageButton
            android:id="@+id/imageButton7"
            android:layout_width="119dp"
            android:layout_height="114dp"
            tools:layout_editor_absoluteX="44dp"
            tools:layout_editor_absoluteY="31dp"
            tools:src="@tools:sample/avatars" />

        <TextView
            android:id="@+id/textView5"
            android:background="#456882"
            android:layout_width="157dp"
            android:layout_height="40dp"
            android:text="Additional Notes:"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="16sp"


            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="334dp" />

        <TextView
            android:id="@+id/textView9"
            android:layout_width="157dp"
            android:background="#456882"
            android:layout_height="40dp"
            android:text="Age:"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="16sp"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="250dp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="157dp"
            android:background="#456882"
            android:layout_height="40dp"
            android:layout_marginTop="8dp"
            android:text="Noelle Paraz"
            android:textAppearance="@style/TextAppearance.AppCompat.Body2"
            android:textSize="16sp"
            app:layout_constraintTop_toBottomOf="@+id/imageButton7"
            tools:layout_editor_absoluteX="16dp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="331dp"
            android:layout_height="313dp"
            android:text=""
            android:background="#234C6A"
            app:layout_constraintTop_toBottomOf="@+id/textView5"
            tools:layout_editor_absoluteX="27dp" />

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="44dp"
            android:layout_height="44dp"
            android:background="#456882"
            android:src="@android:drawable/ic_menu_revert" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00E1D4E2"
            android:backgroundTint="#501414"
            android:text="My Provider"
            android:visibility="visible"
            app:strokeColor="#401B1B"
            tools:layout_editor_absoluteX="260dp"
            tools:layout_editor_absoluteY="16dp" />

        <ImageButton
            android:id="@+id/imageButton10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@android:drawable/ic_menu_edit"
            app:layout_constraintStart_toEndOf="@+id/textView9"
            android:background="#456882"
            tools:layout_editor_absoluteY="239dp" />

        <ImageButton
            android:id="@+id/imageButton13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@android:drawable/ic_menu_edit"
            app:layout_constraintStart_toEndOf="@+id/textView5"
            android:background="#456882"

            tools:layout_editor_absoluteY="323dp" />

        <ImageButton
            android:id="@+id/imageButton12"
            android:background="#456882"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@android:drawable/ic_menu_edit"
            app:layout_constraintStart_toEndOf="@+id/textView8"
            tools:layout_editor_absoluteY="145dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>